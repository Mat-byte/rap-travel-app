{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run conscuentas-cuentasui5",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/conscuentas/conscuentas-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/mcccuentasui5/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/conscuentas/cuentasui5",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/conscuentas/cuentasui5\"}"
      },
      "envFile": "${workspaceFolder}/conscuentas/cuentasui5/.env1",
      "preLaunchTask": "Build conscuentas"
    }
  ]
}